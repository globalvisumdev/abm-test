  "browserslist": [
    "IOS >= 15",
    "Safari >= 15",
    "last 5 Chrome versions",
    "last 5 Firefox versions"
  ],
  "dependencies": {
    "dom7": "^4.0.6",
    "framework7": "^8.3.2",
    "framework7-icons": "^5.0.5",
    "material-icons": "^1.13.12",
    "skeleton-elements": "^4.0.1",
    "swiper": "^11.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "babel-loader": "^9.1.3",
    "cpy-cli": "^5.0.0",
    "cross-env": "^7.0.3",
    "postcss-preset-env": "^9.4.0",
    "rollup-plugin-framework7": "^1.2.1",
    "vite": "^5.1.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "workbox-cli": "^7.0.0"
  }
}
